<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>ユーザ名</th>
            <th>メールアドレス</th>
            <th>退会ボタン</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td></td>
        </tr>
        <% end %>
    </tbody>
</table>